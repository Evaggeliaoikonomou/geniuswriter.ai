generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model userApiLimit {
  id        String   @id @default(uuid())
  userId    String
  count     Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model userSubscription {
  id             String   @id @default(uuid())
  userId         String
  subscriptionId String
  periodEnd      DateTime
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
